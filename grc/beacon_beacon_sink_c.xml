<?xml version="1.0"?>
<block>
  <name>Beacon Sink</name>
  <key>beacon_beacon_sink_c</key>
  <category>[beacon]</category>
  <import>import beacon</import>
  <make>beacon.beacon_sink_c($log_period, $ref_ampl, $ref_cnr, $fft_len, $alpha)</make>
  <param>
    <name>Logging Period</name>
    <key>log_period</key>
    <type>float</type>
  </param>
  <param>
    <name>Reference Amplitude</name>
    <key>ref_ampl</key>
    <type>float</type>
  </param>
  <param>
    <name>Reference CNR</name>
    <key>ref_cnr</key>
    <type>float</type>
  </param>
  <param>
    <name>FFT Length</name>
    <key>fft_len</key>
    <type>int</type>
    </param>
  <param>
    <name>Alpha</name>
    <key>alpha</key>
    <type>float</type>
  </param>
  <sink>
    <name>in</name>
    <type>complex</type>
  </sink>
</block>
